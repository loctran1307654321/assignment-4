import java.util.Scanner;
public class EveraAndOld {
    public static void main(String[] args) {
    int n;
    Scanner scanner =new Scanner(System.in);
        System.out.println("Input: ");
    n = scanner.nextInt();
    if(n%2==0)
    {
        System.out.println("Output: "+n+"is an even number");
    }else
    {
        System.out.println("Output: "+n +"is an odd number");
    }
    
    }
}
